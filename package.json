    "eslint": "^8.44.0",
    "eslint-config-next": "13.4.9",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "husky": "^8.0.0",
    "postcss": "8.4.25",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "pretty-quick": "^3.1.3",
    "sharp": "^0.32.2",
    "tailwindcss": "3.3.2",
    "tailwindcss-animate": "^1.0.6",
    "typescript": "5.1.6"
  }
